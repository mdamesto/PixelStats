{
    "routes": [
      {
        "src": "/icons/(.*)",
        "dest": "/public/icons/$1",
        "continue": true
      },
      {
        "src": "/static/(.*)",
        "dest": "/public/static/$1",
        "continue": true
      },
      {
        "src": "/(.*\\.(ico|png|jpg|jpeg|svg|gif|csv))",
        "dest": "/public/$1",
        "continue": true
      },
      {
        "src": "/.*",
        "dest": "/index.html"
      }
    ]
  }